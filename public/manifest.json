{
  "manifest_version": 3,
  "name": "Seo Tool",
  "description": "A SEO Tool chrome extension",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "128": "images/icon-128.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["*://*/*"]
}
